#include <stdio.h>

#define MAXLINE 1000
#define SPACES 5

int main (){
  while((c = getlinee(line, MAXLINE))  > 0 )
    detab
}

/* getline: lee una linea en s, regresa su longitud. */
int getlinee(char s[], int lim){
  int c, i;

  for(i = 0; i < lim-1 && ( c = getchar()) != EOF && c != '\n'; i++)
    s[i] = c;
  if(c == '\n'){
    s[i] = c;
    i++;
  }
  s[i] = '\0';
  return i;
}

void detab(char s[], int lim){
  int i;
  i = 0;
  while(s[i] == '\t')
    for(int j = 0; j < SPACES; j++)
      s[i++] = ' ';
}
